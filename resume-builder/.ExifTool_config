%Image::ExifTool::UserDefined = (
    'Image::ExifTool::XMP::Main' => {
        rms => { # <-- must be the same as the NAMESPACE prefix
            SubDirectory => {
                TagTable => 'Image::ExifTool::UserDefined::rms', # (see the definition of this table below)
            },
        },
    },
);

